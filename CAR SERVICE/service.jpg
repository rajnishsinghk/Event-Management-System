		<script>
			function toTitleCase(str) {
				return typeof str === 'string'
					? str.replace(/\w\S*/g, txt => txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase())
					: str
			}
			const urlSearchParams = new URLSearchParams(window.location.search);
			const params = Object.fromEntries(urlSearchParams.entries());
			const name = document.querySelector('.name');
			const template = params['template'] ? params['template'].replace(/-/g, ' ') : 'brookwood';
			// const buttonInstall = document.querySelector('[data-button-try]');

			name.innerHTML = template;
			// buttonInstall.setAttribute('data-template-name', toTitleCase(template))

			const iframe = document.querySelector('.device-preview iframe');
			const devicePreview = document.querySelector('.device-preview');
			iframe.src = 'https://cdn.pagefly.io/templates/html/' + params['template'] + '.html';

			let lastClickedElement;

			for (const device of document.querySelectorAll('[data-device]')) {
				device.addEventListener('click', function(event) {
					event.preventDefault();

					device.parentElement.querySelectorAll('a').forEach( e => e.classList.remove('selected'));
					device.classList.add('selected')

					const data = device.getAttribute('data-device')

					devicePreview.classList.remove('laptop','tablet','mobile')
					devicePreview.classList.add(data)

					const url = new URL(window.location);
					url.searchParams.set('device', data);
					window.history.pushState({}, '', url)
				})
			};

			if ( params['device'] ) {
				devicePreview.classList.add(params['device'])

				document.querySelector('.selected').parentElement.querySelectorAll('a').forEach( e => e.classList.remove('selected'));
				document.querySelector('[data-device="' + params['device'] + '"]').classList.add('selected')
			}
		</script>
